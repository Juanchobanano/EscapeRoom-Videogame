        <arguments>
          <argument>
            <kind>1</kind>
            <string>var val = ds_map_find_value(iap_data, "type");
switch(val){
   case iap_ev_purchase:
      var map = ds_map_create();
      var purchase_id = ds_map_find_value(iap_data, "index");
      iap_purchase_details(purchase_id, map);
      if(ds_map_find_value(map, "status") == iap_purchased){
          var product_id = ds_map_find_value(map, "product");
          iap_consume(product_id);
      }
      ds_map_destroy(map);
   break;
   case iap_ev_consume:
      var product_id = ds_map_find_value(map, "product");
      if(ds_map_find_value(iap_data, "consumed")){
        switch(product_id){
            case "noads":
                global.noads = true; // Aumentar la cantidad de vidas.
            break;
        }
      }
      
      // Save the no ads.
      var grid1 = ds_grid_create(1,2);
      ds_grid_add(grid1, 0, 0, "noads_boolean");
      ds_grid_add(grid1, 0, 1, noads);
      Save_Data("NoAds", grid1, "single");
   
   break;
}

</string>
          </argument>
        </arguments>
      </action>
